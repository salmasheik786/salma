class Example21
{
	public static void main(String[] args) 
	{
		int[] arr= {3,6,9,4,1,1,3,9,2,1,6,3,3};
		int element=0;
		int maxcount=0;
		for (int i=0;i<arr.length ;i++)
		{
			int cnt=0;
			for (int j=0;j<arr.length ;j++)
			{
				if (arr[i]==arr[j])
				{
					cnt++;
				}
			}
			if (maxcount<cnt)
			{
				maxcount=cnt;
				element=arr[i];
		        System.out.println(element+" is repeating for "+maxcount+" times");
			}
		}
	}
}
			
		
	
      
